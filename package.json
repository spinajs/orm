{
  "name": "@spinajs/orm",
  "version": "1.0.0",
  "description": "SpinaJS Orm module",
  "main": "lib/index.js",
  "private": false,
  "files": [
    "lib/**/*"
  ],
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/spinajs/orm.git"
  },
  "author": "SpinaJS <spinajs@coderush.pl> (https://github.com/spinajs/core)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/spinajs/orm/issues"
  },
  "homepage": "https://github.com/spinajs/orm#readme",
  "devDependencies": {
    "@types/chai": "^4.1.7",
    "@types/glob": "^7.1.1",
    "@types/mocha": "^5.2.7",
    "@types/sinon": "^7.0.13",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "mocha": "^6.1.4",
    "nyc": "^14.1.1",
    "prettier": "^1.18.2",
    "sinon": "^7.3.2",
    "ts-mocha": "^6.0.0",
    "ts-node": "^8.3.0",
    "tslint": "^5.18.0",
    "tslint-circular-dependencies": "^0.1.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-config-standard": "^8.0.1",
    "tslint-no-unused-expression-chai": "^0.1.4",
    "typedoc": "^0.14.2",
    "typescript": "^3.5.3"
  },
  "scripts": {
    "test": "ts-mocha -p tsconfig.json test/**/*.test.ts",
    "coverage": "nyc npm run test",
    "build-docs": "rimraf docs && typedoc --options typedoc.json src/",
    "build": "tsc",
    "prepare": "npm run build",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "nyc": {
    "extension": [
      ".ts",
      ".tsx"
    ],
    "exclude": [
      "**/*.d.ts"
    ],
    "reporter": [
      "html"
    ],
    "all": true
  },
  "dependencies": {
    "@spinajs/configuration": "^1.0.0",
    "@spinajs/di": "^1.0.2",
    "@spinajs/exceptions": "^1.0.0",
    "lodash": "^4.17.14"
  }
}
